<ResourceDictionary
xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:local="clr-namespace:ResourceLibrary">
   <ResourceDictionary.MergedDictionaries>
      <ResourceDictionary Source="ExpressionDark.xaml"/>
      <ResourceDictionary Source="ExpressionLight.xaml"/>
      <ResourceDictionary Source="SciaCheck.xaml"/>      
   </ResourceDictionary.MergedDictionaries>
   
   <RadialGradientBrush        
    x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type local:CustomResources},ResourceId=CompressionPartBrush}" 
    RadiusX="1" RadiusY="1" GradientOrigin="0.7,0.3">
      <GradientStop Color="White" Offset="0" />
      <GradientStop Color="#FFEBD84E" Offset="1" />
   </RadialGradientBrush>

   <RadialGradientBrush        
    x:Key="CompressionPartBrush" 
    RadiusX="1" RadiusY="1" GradientOrigin="0.7,0.3">
      <GradientStop Color="White" Offset="0" />
      <GradientStop Color="#FFEBD84E" Offset="1" />
   </RadialGradientBrush>

</ResourceDictionary>